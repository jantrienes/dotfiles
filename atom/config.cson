"*":
  "atom-beautify":
    python:
      default_beautifier: "black"
      sort_imports: true
  "atom-isort": {}
  "atom-latex":
    preview_after_build: "Do nothing"
  "atom-python-test":
    additionalArgs: "-v --show-capture=all -s"
    outputColored: true
  "atom-test-runner":
    pythonAllCommand: "pytest {relative_path}"
  "autocomplete-python":
    outputDebug: true
    useKite: false
  "autocomplete-python-jedi":
    useSnippets: "required"
  core:
    customFileTypes:
      "source.yaml": [
        "MLProject"
      ]
    disabledPackages: [
      "metrics"
      "autocomplete-python"
    ]
    editor:
      multiCursorOnClick: true
    packagesWithKeymapsDisabled: []
    telemetryConsent: "no"
    titleBar: "custom"
  editor:
    fontSize: 16
    invisibles: {}
    preferredLineLength: 100
    showIndentGuide: true
    softWrap: true
    softWrapAtPreferredLineLength: true
  "file-types":
    MLProject: "source.yaml"
  "formatters-python":
    formatOrder: [
      "black"
      "isort"
    ]
  "fuzzy-finder": {}
  "language-babel":
    autoIndentJSX: true
    suppressTranspileOnSaveMessages: false
  latex:
    buildOnSave: true
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
  "linter-pylint":
    disableTimeout: true
  "linter-shellcheck":
    enableNotice: true
    userParameters: "-x"
  "linter-ui-default": {}
  "markdown-pdf": {}
  "markdown-preview":
    useGitHubStyle: true
  "one-dark-ui": {}
  "python-black-isort":
    onlyWhenAProjectConfigIsFound: false
  "python-isort":
    sortOnSave: true
  script:
    ignoreSelection: true
  "spell-check":
    addKnownWords: true
    excludedScopes: [
      ".support.function.tex"
      "storage.type.function.latex"
      "keyword.control.[a-z]+.latex"
      "support.function.[a-z]+.latex"
    ]
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "source.rst"
      "text.restructuredtext"
      "text.tex.latex"
      "text.tex.latex.beamer"
    ]
    useLocales: false
  "status-bar": {}
  tabs:
    enableMruTabSwitching: false
  "tree-view": {}
  welcome:
    showOnStartup: false
  wordcount:
    extensions: [
      "md"
      "markdown"
      "readme"
      "txt"
      "rst"
      "adoc"
      "log"
      "msg"
      "tex"
    ]
".babel.regexp.source":
  editor:
    preferredLineLength: 100
".css.source.styled":
  editor:
    preferredLineLength: 100
".editorconfig.source":
  editor:
    preferredLineLength: 100
".js.jsx.source":
  editor:
    preferredLineLength: 100
    showIndentGuide: true
".languagebabel.ttlextension":
  editor:
    preferredLineLength: 100
